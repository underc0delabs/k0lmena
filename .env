#Automation Web
BASEURL=https://qarmy.ar/practica/automation
BROWSER=chromium
HEADLESS=true
SLOWMO=0

# Report output root (ah√≠ se guardan screenshots/ y traces/)
REPORT_DIR=src/reports/front
TRACE=on-failure

VIEWPORT_WIDTH=1366
VIEWPORT_HEIGHT=768
LOCALE=es-AR
TIMEZONE=America/Argentina/Mendoza

# Front auto-healing (Playwright)
K0LMENA_AUTO_HEALING=on
K0LMENA_AUTO_HEALING_FLUSH=debounced
K0LMENA_AUTO_HEALING_WRITE_PRETTY=0
K0LMENA_AUTO_HEALING_LOG=0
K0LMENA_AUTO_HEALING_CONSOLE=1
K0LMENA_AUTO_HEALING_HISTORY_PATH=src/front-test/auto-healing/front-history.json
K0LMENA_AUTO_HEALING_MAX_CANDIDATES=12
K0LMENA_AUTO_HEALING_SNAPSHOT_TIMEOUT_MS=250
K0LMENA_AUTO_HEALING_CANDIDATE_TIMEOUT_MS=1500

#Automation APIs
API_BASEURL=https://petstore.swagger.io

#Automation Mobile
BROWSERSTACK_USER=tu_usuario_browserstack
